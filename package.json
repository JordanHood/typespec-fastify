{
  "name": "typespec-fastify",
  "version": "0.2.0",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "repository": {
    "url": "git+ssh://git@github.com/JordanHood/typespec-fastify.git"
  },
  "peerDependencies": {
    "@alloy-js/core": ">=0.20.0",
    "@alloy-js/typescript": ">=0.20.0",
    "@typespec/compiler": ">=1.0.0",
    "@typespec/emitter-framework": ">=0.15.0",
    "@typespec/http": ">=1.0.0",
    "fastify-type-provider-zod": ">=4.0.0",
    "typespec-zod": ">=0.0.0-63",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "fastify-type-provider-zod": {
      "optional": false
    },
    "zod": {
      "optional": false
    },
    "typespec-zod": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "typespec",
    "fastify",
    "emitter",
    "tsp",
    "codegen"
  ],
  "scripts": {
    "build:emitter": "alloy build",
    "build": "pnpm build:emitter && pnpm emit:e2e --silent && pnpm build:emitter",
    "watch": "alloy build --watch",
    "type": "tsc --noEmit -p tsconfig.test.json",
    "test": "pnpm type && pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "pnpm emit:e2e && pnpm run:e2e",
    "emit:e2e": "node eng/scripts/emit-e2e.js",
    "run:e2e": "vitest run --config vitest.config.e2e.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@alloy-js/cli": "0.22.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.8",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "@typespec/compiler": "1.8.0",
    "@typespec/emitter-framework": "0.15.0",
    "@typespec/http-specs": "0.1.0-alpha.30",
    "@typespec/spector": "0.1.0-alpha.22",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "fastify": "5.6.2",
    "fastify-type-provider-zod": "4.0.2",
    "neostandard": "0.12.2",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "typespec-zod": "0.0.0-63",
    "vitest": "4.0.17",
    "zod": "4.3.6"
  }
}
