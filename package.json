{
  "name": "typespec-emitter-fastify",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "peerDependencies": {
    "@alloy-js/core": "0.20.0",
    "@alloy-js/typescript": "0.20.0",
    "@typespec/compiler": "latest",
    "@typespec/emitter-framework": "0.12.0",
    "@typespec/http": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typespec/compiler": "latest",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "alloy build",
    "watch": "alloy build --watch",
    "test": "node --test 'dist/test**/*.test.js'",
    "lint": "eslint src/ test/ --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "format": "prettier .  --write",
    "format:check": "prettier --check ."
  },
  "private": true,
  "packageManager": "npm@11.6.2+sha512.ee22b335fcbc95662cdf3ab8a053daf045d9cf9c6df6040d28965abb707512b2c16fa6c5eec049d34c74f78f390cebd14f697919eadb97756564d4f9eccc4954",
  "dependencies": {
    "@alloy-js/cli": "0.21.0"
  }
}
